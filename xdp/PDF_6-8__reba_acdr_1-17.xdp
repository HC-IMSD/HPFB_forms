                //create the FieldIndex Key   
                if (oChildNode.className === "subform") {
                    presentNodeDefParentKey = oChildNode.name;
                }




                //create the label key
                if (oChildNode.className !== "subform") {
                //nodePropertyJS+=",\n"+ "\tfieldNumber:	\"" +""+"\"" 
                    nodePropertyJS += ",\n" + "\tlabelKey:	\"" + nodeDefLabelKey + "\"";
                    //  node4Resource + = "test";// ",\n" + nodeDefAlias +"\"";
                    var temp = oItem.caption.value.text.value
