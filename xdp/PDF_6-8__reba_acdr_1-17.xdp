                // nodeDefParentKey=oChildNode.name;
                //}
                if (testForRoot(oChildNode)) {
                    nodeDefAlias = "root";
                    nodeDefSom = "xfa.form." + oChildNode.name;
                    nodeDefParentKey = null;
                    nodeDefLabelKey = null;
                    nodeDefDelegate = null;
                    nodeDefFldNumber = 0;
