               <script contentType="application/x-javascript" name="viewModel">
function onClickShowBtn() {
	show();
}

function onClickHideBtn() {
	hide();
}


function show() {
	Button.disable(sfInstructionBtns.btnViewInstructions);
	Button.enable(sfInstructionBtns.btnHideInstructions);
	
	this.resolveNode("sfInstructionsContent").presence = "visible"
	if (isEnglish()) {
		this.resolveNode("sfInstructionsContent.lblInstructionsHeader").presence = "visible";
		this.resolveNode("sfInstructionsContent.lblInstructions_en").presence = "visible";
		this.resolveNode("sfInstructionsContent.lblInstructions_fr").presence = "hidden";
		this.resolveNode("sfInstructionBtns.txtGuideDocFrench").presence = "hidden";
		this.resolveNode("sfInstructionBtns.txtGuideDocEnglish").presence = "visible";
	} else {
		this.resolveNode("sfInstructionsContent.lblInstructionsHeader").presence = "visible";
		this.resolveNode("sfInstructionsContent.lblInstructions_en").presence = "hidden";
		this.resolveNode("sfInstructionsContent.lblInstructions_fr").presence = "visible";
		this.resolveNode("sfInstructionBtns.txtGuideDocFrench").presence = "visible";
		this.resolveNode("sfInstructionBtns.txtGuideDocEnglish").presence = "hidden";
	}
}




function hide() {
	Button.disable(sfInstructionBtns.btnHideInstructions);
	Button.enable(sfInstructionBtns.btnViewInstructions);
	this.resolveNode("sfInstructionsContent").presence = "hidden"
	this.resolveNode("sfInstructionsContent.lblInstructionsHeader").presence = "hidden";
	this.resolveNode("sfInstructionsContent.lblInstructions_en").presence = "hidden";
	this.resolveNode("sfInstructionsContent.lblInstructions_fr").presence = "hidden";
	this.resolveNode("sfInstructionBtns.txtGuideDocFrench").presence = "hidden";
	this.resolveNode("sfInstructionBtns.txtGuideDocEnglish").presence = "hidden";
}

function disableBothButtons() {
	Button.disable(sfInstructionBtns.btnViewInstructions);
	Button.disable(sfInstructionBtns.btnHideInstructions);
}

function configureInstructions() {
    if (viewModel.isShowing()) {
        viewModel.show();
    } else {
        viewModel.hide();
    }
}

function isShowing() {
  return (this.resolveNode("sfInstructionsContent").presence === "visible");
}

function isEnglish() {
	var cLog = script.core.util.Log;
	var cDrugApplModel=script.DrugAppl.model.DrugApplMain;
	var language = cDrugApplModel.getLanguage();
	
	return (language === "en");
}
</script>
