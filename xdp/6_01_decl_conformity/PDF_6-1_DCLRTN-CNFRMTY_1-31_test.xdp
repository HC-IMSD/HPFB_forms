*
*
*
*/
function initialize(){
		
	
	_updateFormLanguage();
	
	console.println("cFormHandler.initialize()... hiding Attestation subform");
	var attestationModule = this.resolveNode("sfPage1.sfAttestation.variables.AttestationModule");
	if (this.resolveNode("sfPage1.sfFormButtons.btnFinalizeBottom").access === "open") {
		attestationModule.hideAttestationBlock();
	} else {
		attestationModule.makeVisible();
	}
	
}







