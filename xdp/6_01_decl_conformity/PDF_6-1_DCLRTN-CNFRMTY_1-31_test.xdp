                  <event activity="change" name="event__change">
                     <script contentType="application/x-javascript">


//app.alert(xfa.event.newText);
//app.alert(xfa.event.prevText);

//if(ddlOrganization.rawValue == null &amp;&amp; ddlStandardNumber.rawValue == null){
var keypressed = xfa.event.commitKey;
if (keypressed != 3){

if (xfa.event.newText != xfa.event.prevText) {
	if (xfa.event.newText == "Yes" || xfa.event.newText == "Oui") {
		this.resolveNode('sfTestingBody.sfHeading._sfCertificationBody').addInstance(1);
		this.resolveNode("script.DrugAppl.control.#variables.FormHandler")._updateFormLanguage();
		if (xfa.host.version &lt; 8) {
			xfa.form.recalculate(1);
		}
		var index = this.parent.index;
		var strOrganization = xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlOrganization").rawValue;
		var strStandardNumber = xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlStandardNumber ").rawValue;
		var strEdition = xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlEdition ").rawValue;
		
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlOrganization").presence = "hidden";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlStandardNumber").presence = "hidden";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlEdition").presence = "hidden";
		
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeOrg").rawValue =  strOrganization;
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeStd").rawValue = strStandardNumber;
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeEdition").rawValue = strEdition;
		
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeOrg").presence = "visible";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeStd").presence = "visible";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeEdition").presence = "visible";
		
		
		var count = this.resolveNode('sfTestingBody.sfHeading._sfCertificationBody').count;
		count = count - 1;
		xfa.resolveNode("sfTestingBody.sfHeading.sfCertificationBody[" + count + "].txtOrganization").rawValue = strOrganization;
		xfa.resolveNode("sfTestingBody.sfHeading.sfCertificationBody[" + count + "].txtStandardNumber").rawValue = strStandardNumber;
		
		
	}else {
		
	
		var count = this.resolveNode('sfTestingBody.sfHeading._sfCertificationBody').count;
		
		for (var i = 0; i &lt; count; i++) {
			var strTempOrganization = xfa.resolveNode("sfTestingBody.sfHeading.sfCertificationBody[" + i + "].txtOrganization").rawValue;
			var strTempStandardNumber = xfa.resolveNode("sfTestingBody.sfHeading.sfCertificationBody[" + i + "].txtStandardNumber").rawValue;
			var index = this.parent.index;
			var strOrganization = xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlOrganization").rawValue;
			var strStandardNumber = xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlStandardNumber ").rawValue;
			
			//var index = this.parent.index;
			//xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlOrganization").access = "open";
			//xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlStandardNumber").access = "open";
			
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlOrganization").presence = "visible";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlStandardNumber").presence = "visible";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].ddlEdition").presence = "visible";
		
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeOrg").presence = "hidden";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeStd").presence = "hidden";
		xfa.resolveNode("sfListStandardsTable.sfList[" + index + "].txtFakeEdition").presence = "hidden";
			
			if (strTempOrganization == strOrganization &amp;&amp; strTempStandardNumber == strStandardNumber) {
				this.resolveNode('sfTestingBody.sfHeading._sfCertificationBody').removeInstance(i);
				
				if (xfa.host.version &lt; 8) {
					xfa.form.recalculate(1);
				}
				break;
			}
			
		}
		
	}
	
	var count = this.resolveNode('sfTestingBody.sfHeading._sfCertificationBody').count;
	if (count &gt; 0) {
		sfTestingBody.sfHeading.presence = "visible";
	} else {
		sfTestingBody.sfHeading.presence = "hidden";
	}
	
}
}
