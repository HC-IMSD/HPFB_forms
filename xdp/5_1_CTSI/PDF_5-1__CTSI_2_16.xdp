	cLog.trace ("inside isOptionAlreadySelected");
	var idx = sfRepeat.classIndex  - 6;
	
	cLog.trace ("inside isOptionAlreadySelected idx: " + idx);
	
	var idx2 ;
	
	
	var nItems ;
	
	if (idx &gt;= 0) {
		var CHANGE_DATE_REPEAT = cFormNodeManager.getFormNodeForKey("sfChangeDateRepeat");
		var reasons_InstanceManager=CHANGE_DATE_REPEAT.getInstanceManager();
	
		nItems    = reasons_InstanceManager.count;
		idx2 = sfChangeDate.classIndex;
		cLog.trace ("nItems1 : " + nItems );
	}
	
	else {
	
		nItems   = xfa.resolveNode("HCeForm.sfPage1.sfRepeat.sfPart1.sfHeader.sfReason.sfChangeDate.instanceManager").count;
		idx2 = xfa.resolveNode("HCeForm.sfPage1.sfRepeat.sfPart1.sfHeader.sfReason.sfChangeDate").classIndex;
		cLog.trace ("nItems2 : " + nItems );
	} 
	
	var selected = false;

	
	cLog.trace ("change CLASS INDEX: " + idx2);

	for(i=0; i&lt;nItems; i++) {
	
			cLog.trace ("Value of I: " + i);
			if (i != idx2) {
			
				cLog.trace ("Before isOptionAlreadySelected...");
			
				var selection ;
		  	
			  	if (idx &gt;= 0) {
			  		cLog.trace ("IDX is &gt;= ZERO!!!!!!!!!!!!!!!!!!!!");
			  		selection = xfa.resolveNode("sfChangeDate[" + i + "].reasonforChangeDropDown").rawValue;
			  		
			  			
			  	}
			  	
			  	else {
			  		if (xfa.resolveNode("HCeForm.sfPage1.sfRepeat.sfPart1.sfHeader.sfReason.sfChangeDate[" + i + "].reasonforChangeDropDown") != null) {
		  			selection = xfa.resolveNode("HCeForm.sfPage1.sfRepeat.sfPart1.sfHeader.sfReason.sfChangeDate[" + i + "].reasonforChangeDropDown").rawValue;
		  			}
			  	}	
				
				cLog.trace ("After isOptionAlreadySelected...");
			
				if (value != null) {
					cLog.trace ("Selection: " + selection);
					cLog.trace ("VALUE: " + value);
					if (selection == value) {
						selected = true;
						this.rawValue = null;
						break;
					}
				}
			}		
		
	}
	
	
	return selected ;
}
