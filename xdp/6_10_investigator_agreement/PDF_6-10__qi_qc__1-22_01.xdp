                  <script contentType="application/x-javascript" name="RegulatoryAddressHandler">
//imports
var cLog = script.core.util.Log;


var cControllerUtil = script.core.util.ControllerUtil;
var cResourceManager = script.core.lang.ResourceManager;
var cFormNodeManager = script.core.model.FormNodeManager;
var cInstanceManagerUtil = script.core.util.InstanceManagerUtil; 
var cTypeEnforcer=script.core.util.TypeEnforcer;
var cTypeChecker=script.core.util.TypeChecker;
var cUiUtil=script.core.util.UiUtil;
var cStringUtil=script.core.util.StringUtil;
var cException = script.core.util.Exception;


//Globals
var YES="Yes";
var NO="No";
var COUNTRY_CANADA="CA";


//Handler name
var SCRIPT_OBJECT_NAME=					"RegulatoryAddressHandler";

//Action Globals
var SPONSOR_COUNTRY_CHANGED_ACTION=		"sponsor_country_changed";
var CLINICAL_COUNTRY_CHANGED_ACTION=		"clinical_country_changed";
var CTSOLD_COUNTRY_CHANGED_ACTION=		"ctsold_country_changed";
var CTSNEW_COUNTRY_CHANGED_ACTION=		"ctsnew_country_changed";
var QUI_COUNTRY_CHANGED_ACTION=		"qui_country_changed";
var REBA_COUNTRY_CHANGED_ACTION=		"reba_country_changed"


//FORM NODES (FN)

var SPONSOR_PROVINCE_FN=cFormNodeManager.getFormNodeForKey("sponsorProvince");
var SPONSOR_POSTAL_CODE_FN=cFormNodeManager.getFormNodeForKey("sponsorPostalCode");

var CLINICAL_PROVINCE_FN=cFormNodeManager.getFormNodeForKey("clinicalProvince");
var CLINICAL_POSTAL_CODE_FN=cFormNodeManager.getFormNodeForKey("clinicalPostalCode");

var CTSOLD_PROVINCE_FN=cFormNodeManager.getFormNodeForKey("ctsoldProvince");
var CTSOLD_POSTAL_CODE_FN=cFormNodeManager.getFormNodeForKey("ctsoldPostalCode");

var CTSNEW_PROVINCE_FN=cFormNodeManager.getFormNodeForKey("ctsnewProvince");
var CTSNEW_POSTAL_CODE_FN=cFormNodeManager.getFormNodeForKey("ctsnewPostalCode");

var QUI_PROVINCE_FN=cFormNodeManager.getFormNodeForKey("quiProvince");
var QUI_POSTAL_CODE_FN=cFormNodeManager.getFormNodeForKey("quiPostalCode");

var REBA_PROVINCE_FN=cFormNodeManager.getFormNodeForKey("rebaProvince");
var REBA_POSTAL_CODE_FN=cFormNodeManager.getFormNodeForKey("rebaPostalCode");




_initialize();


/* Action Handler
*
*
*
*/
function actionPerformed(anEvent)
{
	switch(anEvent.getActionCommand())
	{
		
		case SPONSOR_COUNTRY_CHANGED_ACTION:
				_sponsorInfoCountryChanged(SPONSOR_PROVINCE_FN,SPONSOR_POSTAL_CODE_FN);					
			break;
			
				case CLINICAL_COUNTRY_CHANGED_ACTION:
				_clinicalCountryChanged(CLINICAL_PROVINCE_FN,CLINICAL_POSTAL_CODE_FN);					
			break;
			
				case CTSOLD_COUNTRY_CHANGED_ACTION:
				_ctsoldCountryChanged(CTSOLD_PROVINCE_FN,CTSOLD_POSTAL_CODE_FN);					
			break;
			
				case CTSNEW_COUNTRY_CHANGED_ACTION:
				_ctsnewCountryChanged(CTSNEW_PROVINCE_FN,CTSNEW_POSTAL_CODE_FN);					
			break;
				case QUI_COUNTRY_CHANGED_ACTION:
				_ctsoldCountryChanged(QUI_PROVINCE_FN,QUI_POSTAL_CODE_FN);					
			break;
			
				case REBA_COUNTRY_CHANGED_ACTION:
				_ctsnewCountryChanged(REBA_PROVINCE_FN,REBA_POSTAL_CODE_FN);					
			break;
			 	 
						
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}//switch
}//end fn


/*
* updates the UI based on the submission type that has been selected
*
*
*
*
*/

/*
*	relies on core delegate for updating address and state based on County
*	@param province_formNode- the province droplist to update
*	@param postal_formNode- the postal code textbox to update
*	@returns null
*
*
*
**/
function _applicantCountryChanged(province_formNode,postal_formNode){
	// Update correct labels to province/state and postal/state code and
	// clear the value entered in the postal/state code field
	province_formNode.updateUI();
	postal_formNode.updateUI();
	postal_formNode.getNodeInstance().rawValue = null;	
	//app.alert("test");
}//end fun



function _sponsorInfoCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

function _clinicalCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
		
}//end fn

function _ctsoldCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

function _ctsnewCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

function _quiCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
		
}//end fn

function _rebaCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

</script>
