                  <script contentType="application/x-javascript" name="viewModel">
function onClickShowBtn() {
	show();
}

function onClickHideBtn() {
	hide();
}


function show() {
	Button.disable(btnViewInstructions);
	Button.enable(btnHideInstructions);
	
	this.resolveNode("sfPage1.sfLettertoAssociate").presence = "visible"
	if (isEnglish()) {
		this.resolveNode("sfPage1.sfLettertoAssociate.lblLettertoAssociate").presence = "visible";
		this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference_en").presence = "visible";
		this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference_fr").presence = "hidden";
	} else {
		this.resolveNode("sfPage1.sfLettertoAssociate.lblLettertoAssociate").presence = "visible";
		this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference_en").presence = "hidden";
		this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference_fr").presence = "visible";
	}
}




function hide() {
	Button.disable(btnHideInstructions);
	Button.enable(btnViewInstructions);
	this.resolveNode("sfPage1.sfLettertoAssociate").presence = "hidden"
	this.resolveNode("sfPage1.sfLettertoAssociate.lblLettertoAssociate").presence = "hidden";
	//this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference").presence = "hidden";
	this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference_en").presence = "hidden";
	this.resolveNode("sfPage1.sfLettertoAssociate.lblLetterReference_fr").presence = "hidden";
}

function disableBothButtons() {
	Button.disable(btnViewInstructions);
	Button.disable(btnHideInstructions);
}

function configureInstructions() {
    if (viewModel.isShowing()) {
        viewModel.show();
    } else {
        viewModel.hide();
    }
}

function isShowing() {
  return (this.resolveNode("sfPage1.sfLettertoAssociate").presence == "visible");
}

function isEnglish() {
	var cLog = script.core.util.Log;
	var cDrugApplModel=script.DrugAppl.model.DrugApplMain;
	var language = cDrugApplModel.getLanguage();
	
	return (language == "en");
}
</script>
