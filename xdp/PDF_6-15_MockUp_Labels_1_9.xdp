            <script contentType="application/x-javascript" name="AccessibilityFunctions">/* Xin Lin 
obj1: the object needs the Screen reader text
other inputs must a "draw", or Text Field or Checkbox object, and the caption and text must not be rich text.
*/
function setSRT(obj1)
{
	var srt = "";
	var count = arguments.length;
	var str;
	var staticString = { Line:{ English:"Line ", French:"Ligne "},
						Amount:{ English:"Amount ", French:"Montant "}
					};
	for(var i = 1 ; i &lt; count; i++)
	{	
		var obj = arguments[i];
		
		if( obj.className == "draw" )
		{	
			str = clearString(obj.value.text.value);
			
			if( str.indexOf("?") == (str.length -1 ) || str.indexOf(".") == ( str.length - 1 ) )
				srt += str + " " ;// add a space after
			else if( str.indexOf(":") == ( str.length - 1 ))// remove ":" , add a ". " at end of string
			{
				srt += rTrim(str.substr(0,  str.length-1))+ ". "; 
			}
			else
				srt += str + ". ";
		}
		else if( obj.className == "field" )
		{
			str = clearString(obj.caption.value.text.value);
			if( str.indexOf(":") == ( str.length - 1 ) || str.indexOf(",") == ( str.length - 1 ))
			{
				srt += rTrim(str.substr(0,  str.length-1))+ ". "; 
			}
			else
				srt += str +". ";
		}
		else 
		{	
			if( obj == "Line" || obj== "Line " || obj == "Amount" || obj== "Amount " ){
				var sTxt = rTrim(obj);
				var lang = form1.LANGUAGE.value;
				//console.println( sTxt + lang );
				srt += staticString[sTxt][lang]; 
				
			}
			else
				srt += obj;// a string
		}
	}
	obj1.assist.speak.value = srt; 
//	console.println(obj1.assist.speak.value);
}


function clearString(str)
{
	var newStr = rTrim(str);
	var newStr1 = middleTrim(newStr);
	var newStr2 =  makeUpperCase( newStr1 );
	return newStr2;
	
}
//This function will remove spaces from right side of string
function rTrim(phase)
{
	var tmp = "";
	tmp = phase;
	var lastChar = tmp.substr(tmp.length-1, 1);
	var hasSpace = lastChar.match(/\s/) != null? true:false;
	while( hasSpace)
	{
		tmp = tmp.substr(0, tmp.length-1);
		lastChar = tmp.substr(tmp.length-1, 1);
		hasSpace = lastChar.match(/\s/) != null? true:false;
	}
	return tmp;
	
}
// remove the extra space in the middle of sentence
function middleTrim(phase)
{
	var tmp = "";
	var word ="";
	//console.println("middleTrim()---- phase= " + phase);
	var PhaseArray = phase.split(" ");
	for( var i = 0 ; i &lt; PhaseArray.length; i++)
	{
		word = rTrim(PhaseArray[i]);
		if( i != PhaseArray.length -1 &amp;&amp; word != "" &amp;&amp; word != null )
			tmp += rTrim(PhaseArray[i]) + " ";
		else if( i == PhaseArray.length -1 &amp;&amp; word != "" &amp;&amp; word != null )
			tmp += rTrim(PhaseArray[i]);
	}
	
	return tmp;
	
}
// make the first charactor upcase for string.
function makeUpperCase(str)
{
	var firstChar = str.substr(0,1);
	var firstCharIdx = 0;

	while(firstChar == "" || firstChar == null || firstChar.match(/\s/) != null)
	{
		str = str.substr(++firstCharIdx, str.length);
		firstChar = str.substr(0,1);
	}
	str = firstChar.toUpperCase() + str.substr(1);
	return str;
}
</script>
