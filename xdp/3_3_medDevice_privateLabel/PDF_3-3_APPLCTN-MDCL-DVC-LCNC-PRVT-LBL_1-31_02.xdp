			toggleInstructionsVisiblity(anEvent.getActionCommand());
			break;
					
		default:
			throw cException.createInstance("InstructionsHandler"
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
