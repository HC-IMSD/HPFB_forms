        for (var nNodeCount = 0; nNodeCount &lt; nNodesLength; nNodeCount++) {
        if (oFields.item(nNodeCount).access != "protected")
            oFields.item(nNodeCount).access = "open";
        }
    }
}


function fieldErrorPopup(targetField, msgBundle) {
  	app.alert( cResourceManager.get(msgBundle));
  	
  	if (targetField != null) {
		xfa.host.setFocus(targetField);
	}
	
}


function checkForFaxAndEmail() {
     var optionalFax = this.resolveNode("sfPage1.sfAddresses.sfPart1B.sfPart1BContactDetail.txtFax");
	 var optionalEmail = this.resolveNode("sfPage1.sfAddresses.sfPart1B.sfPart1BContactDetail.txtEmail");
	
     if ((optionalEmail.rawValue== null || optionalEmail.rawValue == "") &amp;&amp; (optionalFax.rawValue == null || optionalFax.rawValue == "")){
     	return false ;
     } else {
      return true;
    }
}
