	var ingredientName_adobeNode=MATERIAL_INGREDIENT_NAME_TXT_FN.getNodeInstance(materialSubform_adobeNode);
	var animalSourcedValue=YES; //always yes
	cLog.trace("***********IngredientName "+ingredientName_adobeNode);
